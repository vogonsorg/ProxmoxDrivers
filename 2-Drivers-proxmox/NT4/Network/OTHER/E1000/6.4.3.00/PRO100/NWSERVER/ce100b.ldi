        {
            DES:    "Message generation"
            TYP:    DEC (1)
            VAL:    0, 1
            DEF:    1
            Out:    'SETATTENTIONSTATE=%s'
        }

        PR CHECKLINKINTERVAL HID
        {
            DES:    "Time between link checks"
            TYP:    DEC (2)
            VAL:    1-30
            DEF:    4
