        ;@#         'Intel~ PRO/100 VM Network Connection',
        ;@#         'Intel~ PRO/100 VE Network Connection',
        ;@#         'Intel~ PRO/100 VM Network Connection',
        ;@#         'Intel~ PRO/100 ICH6 Lan Reserved',
        ;@#         'Intel~ PRO/100 ICH6 Lan Reserved'

        ;@# Client: E100BODI.COM, CE100B.LAN, E100B.LAN

        ;@# Alias:  CE100B.LAN, E100B.LAN

        FILE: CE100B.LAN

        ;comment in the next line to copy IANS.LAN
        OF: IANS.lan

        PR SLOT REQ
        {
            HELP:  $CE100B_3
            TYP:  DEC (5)
            VAL:   1 - 65535
            DEF:  UND
        }

        PR SPEED OPT
        {
            DES:   "Speed"
            TYP:  DEC (3)
            HELP:  $CE100B_4
            VAL:   0, 10, 100
            DEF:   0
            OUT:   'SPEED=%s'
        }

        PR FORCEDUPLEX OPT
        {
            DES:   "Force Duplex"
            HELP:  $CE100B_5
            TYP:  DEC (1)
            VAL:   0, 1, 2
            DEF:   0
            OUT:   'FORCEDUPLEX=%s'
        }

        PR IOMAPMODE OPT
        {
            DES:   "IO Map Mode"
            HELP:  $CE100B_6
            TYP:  DEC (1)
            VAL:   0, 1
            DEF:   0
            OUT:   'IOMAPMODE=%s'
        }

        FR FrameSelect
        {
            HELP:   $CE100B_7
            CD:     $CE100B_8
            CH:     'Ethernet_802.2'
            CD:     $CE100B_9
            CH:     'Ethernet_802.3'
            CD:     $CE100B_10
            CH:     'Ethernet_SNAP'
            CD:     $CE100B_11
            CH:     'Ethernet_II'
            DEF:    1,2,3,4
        }

        ; to include TXTHRESHOLD, please uncomment out this line
        ; PR TXTHRESHOLD OPT
        ; and comment out this one.
        PR TXTHRESHOLD HID
        {
            DES:    "Transmit Threshold"
            Help:   $CE100B_12
            TYP:    DEC (3)
            VAL:    0-200
            DEF:    0
            Out:    'TXTHRESHOLD=%s'
        }

        PR RXFIFO HID
        {
            DES:    "Receive FIFO"
            TYP:    DEC (2)
            VAL:    0-15
            DEF:    8
            Out:    'RXFIFO=%s'
        }

        PR TXFIFO HID
        {
            DES:    "Transmit FIFO"
            TYP:    DEC (2)
            VAL:    0-15
            DEF:    8
            Out:    'TXFIFO=%s'
        }

        PR ADAPTIVEIFS HID
        {
            DES:    "Adaptive IFS"
            TYP:    DEC (1)
            VAL:    0, 1
            DEF:    1
            Out:    'ADAPTIVEIFS=%s'
        }

        PR IFS HID
        {
            DES:    "IFS"
            TYP:    DEC (3)
            VAL:    0-254
            DEF:    0
            Out:    'IFS=%s'
        }

        PR TXDMACOUNT HID
        {
            DES:    "Transmit DMA Count"
            TYP:    DEC (3)
            VAL:    0-127
            DEF:    0
            Out:    'TXDMACOUNT=%s'
        }

        PR RXDMACOUNT HID
        {
            DES:    "Receive DMA Count"
            TYP:    DEC (3)
            VAL:    0-127
            DEF:    0
            Out:    'RXDMACOUNT=%s'
        }

        PR PHYADDRESS HID
        {
            DES:    "PHY Address"
            TYP:    DEC (2)
            VAL:    0-32
            DEF:    1
            Out:    'PHYADDRESS=%s'
        }

        PR ENHANCED HID
        {
            DES:    "Enhanced  Operation"
            TYP:    DEC (1)
            VAL:    0, 1
            DEF:    1
            Out:    'ENHANCED=%s'
        }

