; NET8254x.Din
; $Revision: 1.7 $
;
; Intel(R) PRO/1000 Gigabit Server Adapter
;
;   Version 6.2.1.0
;
[version]
Signature   = "$Windows NT$"
SetupClass  = BASE
Provider    = %Intel%

[Manufacturer]
%Intel%     = Intel

[ControlFlags]

[Intel]

;-----------------------------------------------------------------------------
; DestinationDirs
;
[DestinationDirs]
DefaultDestDir          = 11
e1000.DelFiles          = 12
CoInstaller.DelFiles    = 11
UnInstall.DelFiles      = 11

;----------------------------------------------------------------------------
; Uninstall
[8254x.Uninstall]
DelFiles=e1000.DelFiles, CoInstaller.DelFiles, UnInstall.DelFiles
DelReg=8254x.DelReg

[e1000.DelFiles]
e1000nt5.sys,,,1

[CoInstaller.DelFiles]
IntelNic.dll,,,1
e1000msg.dll,,,1
EtCoInst.dll,,,1
NicCo32.dll,,,1
NicIn32.dll,,,1
EtCo32.dll,,,1
NicCo.dll,,,1
NicCo2.dll,,,1
NicInst.dll,,,1
NicInstG.dll,,,1
NicEtCo.dll,,,1

[UnInstall.DelFiles]
Prounstl.exe,,,1
e1000nt5.din,,,1

[8254x.DelReg]
HKLM,Software\Microsoft\Windows\CurrentVersion\Uninstall\PROSet
HKLM,Software\INTEL\Prounstl
HKLM,Software\INTEL\NIC

;-----------------------------------------------------------------------------
; Localizable Strings
;
[Strings]
Intel                    = "Intel"
				 
